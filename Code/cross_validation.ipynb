    "# calculates the misclassification rate of a model\n",
    "function misclassification(X, y, w)\n",
    "    n = size(X,1)\n",
    "    origY = toBool(y, 3600)\n",
    "    newY = toBool(y, 0)\n",
    "    mis = 0\n",
    "    for i in 1:n\n",
    "        if(origY[i] != newY[i])\n",
    "            mis += 1/n\n",
    "        end\n",
    "    end\n",
    "    return round(mis, digits=4)\n",
    "end"
   ]
