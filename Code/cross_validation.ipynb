    "    :start_station_id,\n",
    "    :user_type,\n",
    "    :gender,\n",
    "    :week_day\n",
    "]\n",
    "\n",
    "cats_sets = [unique(data[:, label]) for label in cat_labels]\n",
    "xCat = hcat([onehot(data[:, cat_labels[i]], cats_sets[i]) for i in 1:size(cat_labels, 1)]...)\n",
    "X = hcat(convert(Matrix, xCat), xReal)"
