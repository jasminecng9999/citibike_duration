   ],
   "source": [
    "#determines if each entry is above or below the cutoff in a vector \n",
    "function toBool(y, cutoff)\n",
    "    for i in 1:length(y)\n",
    "        if y[i]<= cutoff\n",
    "            y[i]=-1\n",
    "        else\n",
    "            y[i]=1\n",
    "        end\n",
    "    end\n",
    "    return y\n",
    "end"
   ]
