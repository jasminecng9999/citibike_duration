   "source": [
    "#y MUST be a one dimensional vector\n",
    "function logReg(x, y)\n",
    "    yNew = toBool(y, 3600)\n",
    "    n = length(yNew)\n",
    "    loss = 1/n * LogisticLoss()\n",
    "    lambda = 0.5\n",
    "    reg = lambda * QuadReg()\n",
    "    w = proxgrad(loss, reg, x, yNew, maxiters=1000)\n",
    "    return w\n",
    "end"
